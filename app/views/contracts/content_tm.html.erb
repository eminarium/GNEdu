  </tr>
  <tr>
    <td style="text-align: left; padding-left: 5px">Kursy</td>
    <td style="text-align: left;"><%= @contract.group.subject.subjectFullName %></td>
    <td style="text-align: left; padding-left: 5px">Wagty</td>
    <td style="text-align: left;"><%= @contract.group.lesson_time.lessonTimeTitle %></td>
