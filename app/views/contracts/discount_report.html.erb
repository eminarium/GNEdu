<div class="wrapper wrapper-content animated fadeInRight">
  <div class="row">
    <div class="col-lg-12">
      <div class="ibox float-e-margins">
        <div class="ibox-title">
          <h5>Arzanladyş Ulanylan Şertnamalar Sanawy</h5>
          <div class="ibox-tools">
            <a class="collapse-link">
              <i class="fa fa-chevron-up"></i>
            </a>
            <a class="dropdown-toggle" data-toggle="dropdown" href="#">
              <i class="fa fa-wrench"></i>
            </a>
            <ul class="dropdown-menu dropdown-user">
              <li><a href="#">Config option 1</a>
              </li>
              <li><a href="#">Config option 2</a>
              </li>
            </ul>
          </div>
        </div>
        <div class="ibox-content">

          <div class="table-responsive">
            <table class="table table-striped table-bordered table-hover dataTables-example" >
              <thead>
              <tr>
                <th>Şertnama ID</th>
                <th>Okuwçy ID</th>
                <th>Ady</th>
                <th>Familiýasy</th>
                <th>Topary (<%= Season.find_by_id(session[:active_season_id]).seasonTitle %>)</th>
                <th>Tapgyry</th>
                <th>Arzanladyş</th>
                <th>Görnüşi</th>
                <th>Bellik</th>
              </tr>
              </thead>

              <tbody>
              <% @current_season_discount_contracts.each do |csc| %>
                <% if (!csc.discount_id.nil?) %>
                      <tr class="gradeA">
                        <td><%= link_to csc.id, csc %></td>
                        <td><%= link_to csc.student.id, csc.student %></td>
                        <td><%= csc.student.fName %></td>
                        <td><%= csc.student.lName %></td>
                        <td><%= link_to csc.group.groupTitle, csc %></td>
                        <td><%= csc.season.seasonNo %></td>
                        <td><%= csc.discount.discountPercent %></td>
                        <td><%= csc.discount.discountName %></td>
                        <td><%= csc.discountReason %></td>
                      </tr>
                <% end %>
              <% end %>
              </tbody>
              <tfoot>
              <tr>
                <th>Şertnama ID</th>
                <th>Okuwçy ID</th>
                <th>Ady</th>
                <th>Familiýasy</th>
                <th>Topary (<%= Season.find_by_id(session[:active_season_id]).seasonTitle %>)</th>
                <th>Tapgyry</th>
                <th>Arzanladyş</th>
                <th>Görnüşi</th>
                <th>Bellik</th>
              </tr>
              </tfoot>
            </table>
          </div>

        </div>
      </div>
    </div>
  </div>
</div>

