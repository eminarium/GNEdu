<div class="wrapper wrapper-content animated fadeInRight">
  <div class="row">
    <div class="col-lg-12">
      <div class="ibox float-e-margins">
        <div class="ibox-title">
          <h5>Nurnabat Hasabat</h5>
          <div class="ibox-tools">
            <a class="collapse-link">
              <i class="fa fa-chevron-up"></i>
            </a>
            <a class="dropdown-toggle" data-toggle="dropdown" href="#">
              <i class="fa fa-wrench"></i>
            </a>
            <ul class="dropdown-menu dropdown-user">
              <li><a href="#">Config option 1</a>
              </li>
              <li><a href="#">Config option 2</a>
              </li>
            </ul>
          </div>
        </div>
        <div class="ibox-content">

          <div class="tabs-container">
            <ul class="nav nav-tabs">
              <li class="active">
                <a data-toggle="tab" href="#tab-students-all"> Ähli Diňleýjiler  </a>
              </li>
              <li class="">
                <a data-toggle="tab" href="#tab-students-returned"> Tölegi Gaýtarylanlar  </a>
              </li>
              <li class="">
                <a data-toggle="tab" href="#tab-students-reserved"> Rezerwler  </a>
              </li>
            </ul>

            <div class="tab-content">

              <div id="tab-students-all" class="tab-pane active">

                <div class="panel-body">


          <div class="table-responsive">
            <table class="table table-striped table-bordered table-hover dataTables-example" >
              <thead>
              <tr>
                <th>ID</th>
                <th>Şertnama №</th>
                <th>Açylan Senesi</th>
                <th>Üýtgedilen Senesi</th>
                <th>Diňleýjiniň maglumatlary</th>
                <th>Topary</th>
                <th>Rezerw</th>
                <th>Gaýtarylan</th>
                <th>Töleg Görnüşi</th>
                <th>Töleg Mukdary</th>
                <th>Şertnama Bellikler</th>
                <th>Diňleýji Bellikler</th>
              </tr>
              </thead>

              <tbody>
              <% @report_contracts.each do |rc| %>
                  <tr class="gradeA">
                    <td><%= rc.id %></td>
                    <td><%= rc.seasonContractNo %></td>
                    <td><%= rc.created_at.strftime("%d.%m.%y") %></td>
                    <td><%= rc.updated_at.strftime("%d.%m.%y") %></td>
                    <td><%= rc.student.fName+" "+rc.student.lName+" "+rc.student.patronymic %></td>
                    <td>
                      <% if rc.group %>
                          <%= rc.group.groupTitle %>
                      <% end %>
                    </td>
                    <td>
                      <% if (rc.isReserve) %>
                          HAWA
                      <% else %>
                          ÝOK
                      <% end %>
                    </td>
                    <td>
                      <% if (rc.isMoneyReturned) %>
                          HAWA
                      <% else %>
                          ÝOK
                      <% end %>
                    </td>
                    <td>
                      <% if rc.payment_type %>
                          <%= rc.payment_type.paymentTypeName %>
                      <% end %>
                    </td>
                    <td><%= rc.amountPaid %></td>
                    <td><%= rc.notes %></td>
                    <td><%= rc.student.notes %></td>
                  </tr>
              <% end %>
              </tbody>
              <tfoot>
              <tr>
                <th>Şertnama №</th>
                <th>Açylan Senesi</th>
                <th>Üýtgedilen Senesi</th>
                <th>Diňleýjiniň maglumatlary</th>
                <th>Topary</th>
                <th>Rezerw</th>
                <th>Gaýtarylan</th>
                <th>Töleg Görnüşi</th>
                <th>Töleg Mukdary</th>
                <th>Şertnama Bellikler</th>
                <th>Diňleýji Bellikler</th>
              </tr>
              </tfoot>
            </table>
          </div>
        </div>
       </div>


