<%#= @tmp %>

<h1>Listing attendances</h1>

<table>
  <thead>
    <tr>
      <th>Contract</th>
      <th>Attendancedate</th>
      <th>Attendancelist</th>
      <th>Attendancenotes</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @attendances.each do |attendance| %>
      <tr>
        <td><%= attendance.contract %></td>
        <td><%= attendance.attendanceDate %></td>
        <td><%= attendance.attendanceList %></td>
        <td><%= attendance.attendanceNotes %></td>
        <td><%= link_to 'Show', attendance %></td>
        <td><%= link_to 'Edit', edit_attendance_path(attendance) %></td>
        <td><%= link_to 'Destroy', attendance, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Attendance', new_attendance_path %>
