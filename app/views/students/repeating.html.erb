<div class="wrapper wrapper-content animated fadeInRight">
  <div class="row">
    <div class="col-lg-12">
      <div class="ibox float-e-margins">
        <div class="ibox-title">
          <h5>Gaýtalanýan Diňleýjiler Sanawy</h5>
          <div class="ibox-tools">
            <a class="collapse-link">
              <i class="fa fa-chevron-up"></i>
            </a>
            <a class="dropdown-toggle" data-toggle="dropdown" href="#">
              <i class="fa fa-wrench"></i>
            </a>
            <ul class="dropdown-menu dropdown-user">
              <li><a href="#">Config option 1</a>
              </li>
              <li><a href="#">Config option 2</a>
              </li>
            </ul>
          </div>
        </div>
        <div class="ibox-content">

          <div class="table-responsive">
            <table class="table table-striped table-bordered table-hover dataTables-example" >
              <thead>
              <tr>
                <th>ID</th>
                <th><u>Diňleýjiniň maglumatlary</u></th>
                <th><u>Doglan Senesi</u></th>
                <th><u>Gaýtalanýan sany</u></th>
              </tr>
              </thead>

              <tbody>
              <% @duplicate_students.each do |ds| %>
                  <tr class="gradeA">
                    <td>
                      <% if (ds.fName) %>
                      <% Student.where(:fName => ds.fName, :lName => ds.lName, :patronymic => ds.patronymic).each do |ss|  %>
                          <% if !Contract.where(:student_id => ss.id).blank? %>
                            <%= link_to ss.id, ss %> (<%= link_to Contract.where(:student_id => ss.id)[0].id, Contract.where(:student_id => ss.id)[0] %>) /
                          <% else %>
                            <%= link_to ss.id, ss %> (ÝOK) /
                          <% end %>
                      <% end %>
                      <% end %>
                    </td>
                    <td><%= ds.fName+" "+ds.lName+" "+ds.patronymic %></td>
                    <td><%= ds.count %></td>
                  </tr>
              <% end %>
              </tbody>
              <tfoot>
              <tr>
                <th>ID</th>
                <th><u>Diňleýjiniň maglumatlary</u></th>
                <th><u>Doglan Senesi</u></th>
                <th><u>Gaýtalanýan sany</u></th>
              </tr>
              </tfoot>
            </table>
          </div>

        </div>
      </div>
    </div>
  </div>
</div>

